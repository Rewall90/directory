    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/postgres": "^0.10.0",
